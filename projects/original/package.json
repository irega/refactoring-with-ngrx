{
  "name": "questions-manager",
  "version": "0.0.0",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "clean": "rimraf node_modules dist coverage",
    "clean:dist": "rimraf dist",
    "clean:install": "npm run clean && npm install",
    "build": "npm run clean:dist && ng build --prod --output-hashing=none",
    "build:tailwind": "./node_modules/.bin/tailwind build ./src/tailwind-build.css -c ./tailwind.config.js -o ./src/styles.scss",
    "serve": "http_server --gzip",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:single-run": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.3",
    "@angular/common": "7.1.3",
    "@angular/compiler": "7.1.3",
    "@angular/core": "7.1.3",
    "@angular/elements": "7.1.3",
    "@angular/forms": "7.1.3",
    "@angular/http": "7.1.3",
    "@angular/platform-browser": "7.1.3",
    "@angular/platform-browser-dynamic": "7.1.3",
    "@angular/router": "7.1.3",
    "core-js": "2.6.0",
    "rxjs": "6.2.2",
    "safe-pipe": "^0.3.0",
    "zone.js": "0.8.26",
    "angular-in-memory-web-api": "0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.0",
    "@angular/cli": "7.1.2",
    "@angular/compiler-cli": "7.1.3",
    "@angular/language-service": "7.1.3",
    "@types/jasmine": "3.3.1",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.12",
    "codelyzer": "4.5.0",
    "http_server": "1.0.12",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "ngx-build-plus": "^7.7.6",
    "protractor": "5.4.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tailwindcss": "^1.5.2",
    "typescript": "3.1.1"
  }
}
